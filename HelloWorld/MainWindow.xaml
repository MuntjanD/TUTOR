        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Button Content="Button" HorizontalAlignment="Left" Margin="405,216,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="162" Width="279">
